{
  "manifest_version": 2,
  "name": "Radio Stream Player",
  "version": "1.4",
  "description": "Plays and stops a radio stream",
  "permissions": ["activeTab"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}