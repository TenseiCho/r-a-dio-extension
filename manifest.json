{
    "manifest_version": 2,
    "name": "Radio Stream Player",
    "version": "1.1",
    "description": "Plays and stops a radio stream",
    "permissions": ["activeTab"],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "icon.png"
      }
    }
  }